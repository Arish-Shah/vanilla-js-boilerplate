  "bugs": {
    "url": "https://github.com/Arish-Shah/vanilla-js-boilerplate/issues"
  },
  "homepage": "https://github.com/Arish-Shah/vanilla-js-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/runtime-corejs2": "^7.9.2",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.3"
  }
}
