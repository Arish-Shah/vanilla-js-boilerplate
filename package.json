{
  "name": "vanilla-js-boilerplate",
  "version": "1.0.0",
  "description": "Vanilla JavaScript boilerplate with Parcel",
  "main": "index.js",
  "scripts": {
    "predev": "rm -rf dist/ .cache/",
    "dev": "parcel public/index.html -p 3000",
    "build": "parcel build public/index.html --out-dir build --public-url ./",
    "start": "npm run dev"
  },
